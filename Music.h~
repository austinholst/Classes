//Music.h
#include <iostream>
#include "Media.h"

using namespace std;

class Music: public Media {
 public:
  Music(char* title, char* artist, int year, int duration, char* publisher);
  virtual char* getArtist();
  virtual char* getPublisher();
  virtual int getDuration();
  ~Music();
 private:
  char* artist;
  char* publisher;
  int duration;
};
